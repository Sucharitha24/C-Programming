#include<stdio.h>
main()
{
	printf("choose your item : \n1. pizza,Rs.239\n2. burger,Rs.129\n3. pasta,Rs.179\n4. French freies,Rs.99\n5. Sandwich,Rs.149");
	int choice;
	scanf("%d",&choice);
	switch(choice)
	{
		case 1:
			printf("You choosed pizza of Rs.239");
			break;
		case 2:
			printf("You choosed Burger of Rs.129");
			break;
		case 3:
			printf("You choosed Pasta of Rs.179");
			break;
		case 4:
			printf("You chooseed French fries of Rs.99");
			break;
		case 5:
			printf("You choosed Sandwich of Rs.149");
			break;
		default :
			printf("Invalid choice");
	}
}
