#include<stdio.h>
main()
{
	int i,j,k,l,m;
	for(i=1;i<5;i++)
	{
		for(j=0;j<i;j++)
	    {
		    for(k=0;k<j;k++)
		    {
		    	for(l=0;l<k;l++)
		    	{
		    	    for(m=0;m<l;m++)
					{
						printf("1",m);
					}	
		    		printf("0",l);
				}
		    	printf("1",k);
			}
	    	printf("0",j);
		}
		printf("1\n",i);
	}
}
